{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@*": ["src/*"],
      "@components": ["src/components"],
      "@pages": ["src/pages"],
      "@redux": ["src/redux"],
      "@locales": ["src/locales"],
      "@domain": ["src/domain"],
      "@assets": ["src/assets"],
      "@containers": ["src/containers"],
      "@constants": ["src/constants"],
      "@hooks": ["src/hooks"],
      "@utils": ["src/utils"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/FundingRounds/TabComponent",
    "src/pages/_error.js",
    "src/components/table.js",
    "src/components/Slider/Arrow.js",
    "src/containers/HomeContainer/components/SessionWebInvestigTime/FlipDown/index.js"
  ],
  "exclude": ["node_modules"]
}
